ul.skills 
  each skill in skills
    each skillArray, skillName in skill
      li.skills__item
        .skill-section
          .skill-section__title= skillName
          ul.skill-section__list
            each skillTitle in skillArray
              li.skills-section__item
                .skill
                  svg.skill__circle
                    circle( r="55" cx="75" cy="75" class="skill__ring")
                    circle( r="65" cx="75" cy="75" class="skill__level")
                  .skill__name= skillTitle